<script>
    var chart = nv.models.discreteBarChart()
            .x(function(d) { return d.label })
            .y(function(d) { return d.value })
            .staggerLabels(true)
            .tooltips(true)
            .showValues(false)
            .transitionDuration(1000)
            .color(<%-JSON.stringify(colors)%>);

    var data = [{ key: 'Bar Chart test', values: <%-JSON.stringify(data)%> }];
    d3.select('#dataviz-container svg').datum(data).call(chart);
</script>